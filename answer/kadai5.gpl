#!/usr/bin/gnuplot

set term pngcairo
set output "kadai5.png"

set xrange[-0.05:1.05]
set yrange[-0.05:1.05]
set size square
unset xtics
unset ytics
set multiplot layout 2,3

set title 'level 0'
plot "kadai5_0.dat" w l t ""

set title 'level 1'
plot "kadai5_1.dat" w l t ""

set title 'level 2'
plot "kadai5_2.dat" w l t ""

set title 'level 3'
plot "kadai5_3.dat" w l t ""

set title 'level 4'
plot "kadai5_4.dat" w l t ""

set title 'level 5'
plot "kadai5_5.dat" w l t ""

